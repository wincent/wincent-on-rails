%h1 Login
= form_for @session do |f|
  = f.hidden_field :original_uri
  = f.email_field :email, label: 'Email address', required: true
  = f.password_field :passphrase, required: true
  = f.submit 'Log in'
.links
  = link_to 'Sign up', new_user_path
  = link_to 'Forgot your passphrase?', new_reset_path
